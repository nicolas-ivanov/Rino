#!/bin/bash #this line is for highlighting purpose only
########################################################################
# A collection of marked user commands for the following commands types:
# a_call	call to smb
# a_sms		send sms to smb
# a_email	send email to smb
# a_search	serch smt on the web
# a_site	launch a site in browser
# a_alarm	set an alarm for particular time
# a_balance	check how much money you have on your account
########################################################################


# A_CALL section #######################################################

# with name or number

a_call	позвони		# кому позвонить?
a_call	васе

a_call	позвони		# кому позвонить?
a_call	василию ивановичу

a_call	позвони		# кому позвонить?
a_call	василию ивановичу петрову

a_call	позвони		# кому позвонить?
a_call	100

a_call	позвони		# кому позвонить?
a_call	по номеру 100

a_call	позвони		# кому позвонить?
a_call	по телефонному номеру 100


# with a_call + name or number

a_call	позвони		# кому позвонить?
a_call	позвони васе

a_call	позвони		# кому позвонить?
a_call	позвони василию ивановичу

a_call	позвони		# кому позвонить?
a_call	позвони василию ивановичу петрову

a_call	позвони		# кому позвонить?
a_call	позвони 100

a_call	позвони		# кому позвонить?
a_call	позвони по номеру 100

a_call	позвони		# кому позвонить?
a_call	позвони по телефонному номеру 100


# with ambiguous "набери" + name or number

a_call	позвони		# кому позвонить?
a_call	набери васю

a_call	позвони		# кому позвонить?
a_call	набери василия ивановича

a_call	позвони		# кому позвонить?
a_call	набери василия ивановича петрова

a_call	позвони		# кому позвонить?
a_call	набери 100

a_call	позвони		# кому позвонить?
a_call	набери номер 100

a_call	позвони		# кому позвонить?
a_call	набери телефонный номер 100



###
# cancellation, 1 s_change word

a_call	позвони			# кому?
a_call	нет позвони васе

a_call	позвони			# кому?
a_sms	нет напиши смс

a_call	позвони			# кому?
a_email	нет напиши письмо

a_call	позвони			# кому?
a_search	нет найди мне статью

a_call	позвони			# кому?
a_site	нет открой мне сайт

a_call	позвони			# кому?
a_alarm	нет поставь будильник

a_call	позвони			# кому?
a_balance	нет покажи баланс


# cancellation, 2 s_change words

a_call	позвони			# кому?
a_call	хотя нет позвони васе

a_call	позвони			# кому?
a_sms	хотя нет напиши смс

a_call	позвони			# кому?
a_email	хотя нет напиши письмо

a_call	позвони			# кому?
a_search	хотя нет найди мне статью

a_call	позвони			# кому?
a_site	хотя нет открой мне сайт

a_call	позвони			# кому?
a_alarm	хотя нет поставь будильник

a_call	позвони			# кому?
a_balance	хотя нет покажи баланс


# cancellation, 3 s_change words

a_call	позвони			# кому?
a_call	хотя нет лучше позвони васе

a_call	позвони			# кому?
a_sms	хотя нет лучше напиши смс

a_call	позвони			# кому?
a_email	хотя нет лучше напиши письмо

a_call	позвони			# кому?
a_search	хотя нет лучше найди мне статью

a_call	позвони			# кому?
a_site	хотя нет лучше открой мне сайт

a_call	позвони			# кому?
a_alarm	хотя нет лучше поставь будильник

a_call	позвони			# кому?
a_balance	хотя нет лучше покажи баланс







# A_SMS section ########################################################

# with text
a_sms	смс			# что нужно написать?
a_sms	_привет

a_sms	смс			# что нужно написать?
a_sms	_привет _вася

a_sms	смс			# что нужно написать?
a_sms	_привет _как _дела

a_sms	смс			# что нужно написать?
a_sms	_привет _вася _как _дела


# with ambiguous "напиши" and text

a_sms	смс			# что нужно написать?
a_sms	напиши _привет

a_sms	смс			# что нужно написать?
a_sms	напиши _привет _вася

a_sms	смс			# что нужно написать?
a_sms	напиши _привет _как _дела

a_sms	смс			# что нужно написать?
a_sms	напиши _привет _вася _как _дела


# with s_text and text

a_sms	смс			# что нужно написать?
a_sms	с текстом _привет

a_sms	смс			# что нужно написать?
a_sms	с текстом _привет _вася

a_sms	смс			# что нужно написать?
a_sms	с текстом _привет _как _дела

a_sms	смс			# что нужно написать?
a_sms	с текстом _привет _вася _как _дела


# with name

a_sms	смс			# кому?
a_sms	васе

a_sms	смс			# кому?
a_sms	васе петрову

a_sms	смс			# кому?
a_sms	василию ивановичу петрову


# with a_sms + name

a_sms	смс			# кому?
a_sms	смс васе

a_sms	смс			# кому?
a_sms	смс васе петрову

a_sms	смс			# кому?
a_sms	смс василию ивановичу петрову


# with ambiguous "отправь" + name

a_sms	смс			# кому?
a_sms	отправь васе

a_sms	смс			# кому?
a_sms	отправь васе петрову

a_sms	смс			# кому?
a_sms	отправь василию ивановичу петрову


# with ambiguous "отправь" + a_sms and name

a_sms	смс			# кому?
a_sms	отправь смс васе

a_sms	смс			# кому?
a_sms	отправь смс васе петрову

a_sms	смс			# кому?
a_sms	отправь смс василию ивановичу петрову


# with full command

a_sms	смс			# кому?
a_sms	смс васе с текстом _привет

a_sms	смс			# кому?
a_sms	смс василию ивановичу с текстом _привет

a_sms	смс			# кому?
a_sms	смс василию ивановичу петрову с текстом _привет

#####
# complicated text (action keywords included)

a_sms	смс			# что написать?
a_sms	_почему _не _звонишь

a_sms	смс			# что написать?
a_sms	_отправь _сообщение _васе

a_sms	смс			# что написать?
a_sms	_получил _твое _письмо

a_sms	смс			# что написать?
a_sms	_разбуди _меня _завтра _в _7

a_sms	смс			# что написать?
a_sms	_открой _браузер

a_sms	смс			# что написать?
a_sms	_пополни _мой _баланс

a_sms	смс			# что написать?
a_sms	_проверь _баланс


# complicated text (params keywords included)

a_sms	смс			# что написать?
a_sms	_привет _от _саши

a_sms	смс			# что написать?
a_sms	_позвони _на _номер _8 _917 _123 _45 _67

a_sms	смс			# что написать?
a_sms	_sasha _собака _gmail.com

a_sms	смс			# что написать?
a_sms	_зайди _на _yandex.ru

a_sms	смс			# что написать?
a_sms	_завтра _буду _в _7 _часов

#####
# complicated text: action keywords included + a_sms

a_sms	смс			# что написать?
a_sms	напиши _почему _не _звонишь

a_sms	смс			# что написать?
a_sms	напиши _напиши _мне

a_sms	смс			# что написать?
a_sms	напиши _получил _твое _письмо

a_sms	смс			# что написать?
a_sms	напиши _разбуди _меня _завтра _в _7

a_sms	смс			# что написать?
a_sms	напиши _открой _браузер

a_sms	смс			# что написать?
a_sms	напиши _пополни _мой _баланс

a_sms	смс			# что написать?
a_sms	напиши _проверь _баланс


# complicated text: params keywords included + s_text

a_sms	смс			# что написать?
a_sms	напиши _привет _от _саши

a_sms	смс			# что написать?
a_sms	напиши _позвони _на _номер _8 _917 _123 _45 _67

a_sms	смс			# что написать?
a_sms	напиши _sasha _собака _gmail.com

a_sms	смс			# что написать?
a_sms	напиши _зайди _на _yandex.ru

a_sms	смс			# что написать?
a_sms	напиши _завтра _буду _в _7 _часов


# cancellation, 1 s_change word

a_sms	смс			# кому?
a_call	нет позвони васе

a_sms	смс			# кому?
a_sms	нет напиши смс

a_sms	смс			# кому?
